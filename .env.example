JWT_COOKIE_EXPIRE_RESUME=120

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_S3_BUCKET_NAME=

SMTP_HOST=
SMTP_PORT=
SMTP_EMAIL=
SMTP_PASSWORD=
FROM_EMAIL=
FORM_NAME=

SERVER_HOST=[gate address]
SERVER_USERNAME=[application server node id. Warning: it is not the username. Format is XXXXX-YYYY. XXXXX part is node id.]
SSH_PRIVATE_KEY=[local path to key on personal computer]
JELASTIC_API_TOKEN=[located on Downloads folder on PC]