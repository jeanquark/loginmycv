MONGO_URI_ONLINE=

LOGINMYCV_MAX_FILE_UPLOAD_SIZE_IN_BYTES=10000000
LOGINMYCV_USER_DEFAULT_ALLOCATED_SPACE_IN_BYTES=10000000

JWT_SECRET_USER=YOUR_SECRET_KEY_GOES_HERE
